class Instrumento:
    def __init__(self, nombre: str, material: str):
        self.nombre = nombre
        self.material = material
    def tipo_sonido(self):
        return f"El instrumento {self.nombre} hecho de {self.material} produce un sonido característico."
class Guitarra(Instrumento):
    def tocar_nota(self, nota: str):
        return f"La guitarra toca la nota {nota} con un sonido de cuerdas."
class Piano(Instrumento):
    def tocar_nota(self, nota: str):
        return f"El piano toca la nota {nota} con un sonido armónico de teclas."
if __name__ == "__main__":
    g = Guitarra("Guitarra acústica", "madera")
    p = Piano("Piano clásico", "madera y metal")
    print(g.tipo_sonido())
    print(g.tocar_nota("Do"))
    print("-----")
    print(p.tipo_sonido())
    print(p.tocar_nota("Sol"))
